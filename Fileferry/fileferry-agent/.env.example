# FileFerry Agent - Environment Configuration Example
# Copy this file to .env and fill in your credentials

# Datadog Configuration
DD_ENABLED=false
DD_AGENT_HOST=127.0.0.1
DD_STATSD_PORT=8125

# Microsoft Teams Webhook
TEAMS_WEBHOOK_URL=

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=

# SFTP Configuration (optional defaults)
SFTP_DEFAULT_PORT=22
SFTP_TIMEOUT=30

# FTP Configuration (optional defaults)
FTP_TIMEOUT=30

# Agent Configuration
FILEFERRY_CONFIG_PATH=./fileferry_config.json
FILEFERRY_NUM_WORKERS=3
FILEFERRY_LOG_LEVEL=INFO